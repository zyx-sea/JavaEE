<!--配置sql语句-->
<mapper namespace="product">
    <!--查询多条信息-->
    <select id="findProductAll" parameterType="int" resultType="cn.qd.book.model.Product">
        SELECT * FROM product WHERE open = #{open};
   </select>

</mapper>